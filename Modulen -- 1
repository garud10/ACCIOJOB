//-------------HCF of two Numbers-----------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc= new Scanner(System.in);
                int a= sc.nextInt();
                int b= sc.nextInt();
                int n=0;
                int i=1;
                while(i<=a && i<=b){
                        if(a%i==0 && b%i==0){
                                n=i;
                        }
                        i++;
                }
                System.out.println(n);
	}
}

//----------------Sum of Natural Numbers (Functions)----------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        		static long NumberSum(long n){
                        long s=0;
                        for(long i=1; i<=n; i++){
                                s=s+i;
                                
                                        }
                                return s;
                }
	public static void main (String[] args) throws java.lang.Exception
	{
         Scanner sc= new Scanner(System.in);
                long n=sc.nextLong();
               
                System.out.println(NumberSum(n));
                
	}
}

//----------------Reverse digits of a Number-----------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int temp=0;
                while(n>0){
                        int d=n%10;
                        temp =(temp*10)+d;
                        n=n/10;
                        
                }
                System.out.println(temp);
	}
}


//-----------Power of a Number---------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int a=sc.nextInt();
                int b=sc.nextInt();
                int p=1;
                for(int i=1; i<=b; i++){
                        p=p*a;
                }
                System.out.println(p);
	}
}


//-----------------Largest Number At Least Twice of Others------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc= new Scanner(System.in);
                int n=sc.nextInt();
                int[]a=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                        
                }
                int s=0;
                int f=0;
                for(int i=0; i<n; i++){
                        int count=0;
                        for(int j=0; j<n; j++){
                                if(a[i]>=2*a[j]){
                                        count =count+1;
                                }
                                if(count==n-1){
                                       s=i;
                                        f=1;
                                }
                        }
                }
                if(f==1){
                        System.out.println(s);
                }
                else{
                        System.out.println("-1");
                }
	}
}



//----------------ArmstrongNumber---------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		int n=500;
                int x,d;
               for(int i=1; i<=n; i++){
                       int sum=0;
                       x=i;
                       while(x>0){
                         d=x%10;
                               sum=sum+(d*d*d);
                               x=x/10;
                       }
                       if(sum==i){
                       System.out.println(sum);
                       }
               }







                
	}
}

//------------------PEAK ELEMENT---------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int[]a=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                 int peak=-1;
                if(a[0]>a[1]){
                        peak=0;
                }
                for(int i=1; i<n-1; i++){
                        if(peak==-1 && a[i]>a[i-1] && a[i]>a[i+1]){
                                peak=i;
                                break;
                        }
                }
                if(peak==-1 && a[n-1]>a[n-2]){
                peak=n-1;
                }
                System.out.println(peak);
                        
                
	}
}

//---------------Factorial with loop---------------//


import java.util.*;
import java.io.*;

public class Main {
    public static void main(String args[]) {
        Scanner sc= new Scanner(System.in);
      long n=sc.nextInt();
      long f=1;
      for(long i=n; i>0; i--){
        f=f*i;
      }
           
      System.out.println(f);
            
    }
}


