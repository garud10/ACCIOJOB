//----------------------Two Sum in sorted Array---------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int k=sc.nextInt();
                int a[]=new int [n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                
                 
               // int j=n-1;
                int count =0;
                for(int i=0; i<n; i++){
               for(int j=i+1; j<n; j++){
                        if(a[i]+a[j]==k){
                              System.out.println((i+1)+" "+(j+1));
                                count++;
                                break;
                        }
                        
                }
                        if(count>0){
                                break;
                        }
                }
	}
}

//----------------------First Come First Serve---------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int  n=sc.nextInt();
                int k=sc.nextInt();
                int a[]=new int[n];
                int max=0;
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                        max=Math.max(max,a[i]);
                }
                int arr[]=new int[max+1];
               for(int i=0; i<n; i++){
                   arr[a[i]]++;
               }
                int x=-1;
                 for(int i=0; i<n; i++){
                   if(arr[a[i]]==k){
                           x=a[i];
                           break;
                   }
               }
                
                System.out.println(x);
	}
}

//--------------------Implementing Merge Sort------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        public static void combine(int start,int mid,int end,int a[]){
                ArrayList<Integer> A=new ArrayList<>();
                for(int i=start; i<=mid; i++){
                        A.add(a[i]);
                }
                ArrayList<Integer> B=new ArrayList<>();
               for(int i=mid+1; i<=end; i++){
                        B.add(a[i]);
                }
                ArrayList<Integer> C=new ArrayList<>();
                int i=0;
                int j=0;
                while(i<A.size() && j<B.size()){
                        if(A.get(i)<= B.get(j)){
                                C.add(A.get(i));
                                i++;
                        }
                        else{
                                C.add(B.get(j));
                                j++;
                        }
                }

                while(i<A.size()){
                        C.add(A.get(i));
                        i++;
                }
                 while(j<B.size()){
                        C.add(B.get(j));
                        j++;
                }

                for(int k=0; k<C.size(); k++){
                        a[start+k]=C.get(k);
                }
                
        }
        public static void merg(int start,int end,int a[]){

                if(start==end){
                        return;
                }
                  int mid=(start+end)/2;

                merg(start,mid,a);
                merg(mid+1,end,a);

                combine(start,mid,end,a);
                
        }
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int a[]=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                merg(0,n-1,a);
                
                for(int i=0; i<n; i++){
                        System.out.print(a[i]+" ");
                }
	}
}

//-----------------Counting Inversions-----------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
    public static int merge(int start, int mid, int end, int arr[]){
            int ans=0;
        ArrayList<Integer> a=new ArrayList<Integer>();
        for(int i=start; i<=mid; i++){
            a.add(arr[i]);
        }
        ArrayList<Integer> b=new ArrayList<Integer>();
        for(int i=mid+1; i<=end; i++){
            b.add(arr[i]);
        }

        ArrayList<Integer> c=new ArrayList<Integer>();
       int i=0;
        int j=0;
        while(i<a.size() && j<b.size()){
            if(a.get(i)<=b.get(j)){
                c.add(a.get(i));
                i++;
            }
            else{
                c.add(b.get(j));
                j++;
                ans+=(a.size()-i);
            }
        }
        while(i<a.size()){
            c.add(a.get(i));
            i++;
        }
        while(i<b.size()){
            c.add(b.get(j));
        j++;
        }

        for(int k=0; k<c.size(); k++){
            arr[start+k]=c.get(k);
        }


        return ans;
        
        
    }
    public static int mergesort(int start, int end, int arr[]){

        if(start==end){
            return 0;
        }

        int mid=(start+end)/2;
         int ans=0;

        ans+=mergesort(start,mid,arr);

        ans+=mergesort(mid+1,end,arr);

        ans+=merge(start,mid,end,arr);


        return ans;
        
    }
    
	public static void main (String[] args) throws java.lang.Exception
	{
		//your code here
		Scanner in = new Scanner(System.in);
        int t = in.nextInt();
        while(t>0)
        {
    	    int n = in.nextInt();
    	    int arr[] = new int[n];
    	    
    	    for(int i=0;i<n;i++)
    	    {
    	        arr[i] = in.nextInt();
    	    }
    	    
    	   int inv=mergesort(0,n-1,arr);
    	    
    	    System.out.println(inv);
    	    t--;
        }
	}
}

//-------------------------Quick Sort-1----------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int a[]=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                for(int i=0; i<n; i++){
                        if(a[i]<a[0]){
                                System.out.print(a[i]+" ");
                        }
                }
                 for(int i=0; i<n; i++){
                        if(a[i]==a[0]){
                                System.out.print(a[i]+" ");
                        }
                }
                 for(int i=0; i<n; i++){
                        if(a[i]>a[0]){
                                System.out.print(a[i]+" ");
                        }
                }
	}
}

