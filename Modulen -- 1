//------------------Ptice-----------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{

           Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                String s=sc.next();
                String a="ABC";
                String b="BABC";
                        String c="CCAABB";
                int marka=0;
                int markb=0;
                int markc=0;

                for(int i=0; i<n; i++){
                        if(s.charAt(i)==a.charAt(i%3)){
                                marka++;
                        }
                        if(s.charAt(i)==b.charAt(i%4)){
                                markb++;
                        }
                        if(s.charAt(i)==c.charAt(i%6)){
                                markc++;
                        }
                }
                int max=Math.max(marka,Math.max(markb,markc));
                System.out.println(max);
                if(max==marka){
                        System.out.println("Adrian");
                        
                }
                 if(max==markb){
                        System.out.println("Bruno");
                }
                 if(max==markc){
                        System.out.println("Goran");
                }
                        
                
                















                
	}
}

