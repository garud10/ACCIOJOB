//------------------Maximum Product of Three Elements----------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int a[]=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                Arrays.sort(a);

                int ans1=(a[n-1])*(a[n-2])*(a[n-3]);
                int ans2=(a[0]*a[1]*a[n-1]);
                System.out.println(Math.max(ans1,ans2));
	}
}

//----------------------HRECURS - Hello Recursion-------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        public static int rec(int index,int a[],int n){
                
                 if(index==n){
                  return 0;
                 }
                

            int sum=a[index]+rec(index+1,a,n);
               
                return sum;
        }
        
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int t=sc.nextInt();
                int test=1;
                for(int i=0; i<t; i++){
                        
                        int n=sc.nextInt();
                        int a[]=new int[n];
                        for(int j=0; j<n; j++){
                                a[j]=sc.nextInt();
                        }
                         int sum=rec(0,a,n);

                        System.out.println("Case "+test+": "+sum);
                        test++;
                }
	}
}

//-------------------Recursive Fibonacci---------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        public static int  rec(int n){
                  
                if(n == 1 ){
                        return 0 ;
                }
                if(n == 2){
                        return 1;
                }

                int fand=rec(n-1)+rec(n-2);
               return fand;
        }
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                  int fab=rec(n);
                System.out.println(fab);
	}
}

//------------------Recursive Digit Sum------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        public static int rec(int p){
                int sumOfDigit=0;
                if(p<10){
                        return p ;
                }
                  while(p>0){
                          int digit=p%10;
                          sumOfDigit +=digit;
                          p=p/10;
                  }
                
             
        return rec(sumOfDigit);
        }
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                String n=sc.next();
                int k=sc.nextInt();
                int sd=0;
             for(int i=0; i<n.length(); i++){
                        sd=sd+n.charAt(i)-'0';
             }
                int sup=sd*k;
                                       
               int p=rec(sup );
        System.out.println(p);
	}
}


//--------------------No X--------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        public static String print(String s,int i){
            String main="";
                if(i==s.length()){
                        return "";
                }

                if(s.charAt(i)!='x'){
                      //  char c=s.charAt(i);
                  main=s.charAt(i)+print(s,i+1);
                     //   System.out.println(main);
                }
                else{
                       main=print(s,i+1);
                }

                return main;
        }
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                String s=sc.next();
              System.out.println(print(s,0));
	}
}

//-------------------------PairStar------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        public static String star(String s, int i){
                String main="";
              if(i+1==s.length()){
                      return s.substring(i);
              }
                if(s.charAt(i)==s.charAt(i+1)){
                        main=s.charAt(i)+"*"+star(s,i+1);
                }
                else{
                        main=s.charAt(i)+star(s,i+1);
                }

                return main;
        }
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                String s=sc.next();

                System.out.println(star(s,0));
	}
}

//-------------------------Countabc-------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        public static int rec(int index,String s){
                if(index+3>s.length()){
                        return 0;
                }
                
                if(s.substring(index,index+3).compareTo("abc")==0 || s.substring(index,index+3).compareTo("aba")==0 ){
                   return(1+rec(index+1,s));
                }
                else{
                        return rec(index+1,s);
                }
                
        }
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                String s=sc.next();
                System.out.println(rec(0,s));
                
	}
}

//-------------Array11-------------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
        public static int rec(int index,int a[]){
                if(index==a.length){
                        return 0;
                }
                if(a[index]==11){
                        return (1+rec(index+1,a));
                }
                else{
                        return (rec(index+1,a));
                }
        }
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int a[]=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                System.out.println(rec(0,a));
	}
}

//--------------------Two Sum in an Array---------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                long t=sc.nextLong();
                int []a=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                int ind1=0;
                int ind2=0;
                
                for(int i=0; i<n; i++){
                        for(int j=i+1; j<n; j++){
                                if(a[i]+a[j]==t){
                                       ind1=i;
                                        ind2=j;
                                }
                        }
                        
                }
                System.out.println(ind1+" "+ind2);
	}
}

//-----------------------Maximum Contiguous Subarray Sum---------------------//

import java.util.*;

class Main {
    public static void main(String args[]) {
        Scanner input = new Scanner(System.in);
        int n = input.nextInt();
        int a[] = new int[n];
        for(int i=0;i<n;i++){
            a[i] = input.nextInt();
        }
        Solution ob =new Solution();
        long ans = ob.maxSubarraySum(a,n);
        System.out.println(ans);
    }
}
class Solution{
    public long maxSubarraySum(int a[],int n){
        //Write code here
		int sum = 0;
		int maxSum = Integer.MIN_VALUE;
		
		for(int i = 0; i < n; i++){
			sum += a[i];
			if(maxSum < sum){
				maxSum = sum;
			}
			if(sum < 0) sum = 0;
		}
		return maxSum;
    }
}

//---------------------0-1-2 Sorting-----------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int a[]=new int[n];
                int zero=0;
                int one=0;
                int two=0;
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                        if(a[i]==0){
                                zero++;
                        }
                        else if(a[i]==1){
                                one++;
                        }
                        else{
                                two++;
                        }
                }
                int i=0;
                while(zero>0){
                        a[i]=0;
                        i++;
                        zero--;
                }
               while(one>0){
                        a[i]=1;
                        i++;
                       one--;
                }
                while(two>0){
                        a[i]=2;
                        i++;
                        two--;
                }
                
                for(int j=0; j<n; j++)
                {
                        System.out.print(a[j]+" ");
                }                        
                
               
                       
	}
}

//-------------------Zigzag Conversion----------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                String s=sc.next();
                int k=sc.nextInt();
                if(k==1){
                        System.out.println(s);
                }
                else{
                int count=0;
                String a[]=new String[k];
                boolean flag=true;
                for(int i=0; i<s.length(); i++){
                        if(flag==true){
                                a[count++]+=s.charAt(i);
                                if(count==k-1){
                                        flag=false;
                                }
                        }
                     else{
                              a[count--]+=s.charAt(i);
                              if(count==0){
                                      flag=true;
                              }
                      }
                }
                String res="";
                for(int i=0; i<a.length; i++){
                        res+=a[i];
                }
                res=res.replace("null","");
                System.out.println(res);
                }
	}
}

// --------------------Maximum Consecutive Ones------------------//

import java.io.*;
import java.util.*;

class Solution{
     public static int maxConsecutiveOnes(int[] arr, int n) {
        // Your code here
		 int start = 0;
		 int end = 0;
		int zero = 0;
		 int max = 0;
		 while(end < n){
			 if(arr[end] == 0) zero++;
			 end++;

			 while(start < end && zero > 0){
				 if(arr[start] == 0) zero--;
				 start++;
			 }

			 if(zero == 0) max = Math.max(max,(end - start));
		 }
		 return max;
}
}
public class Main {

    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for (int i = 0; i < n; i++)
            arr[i] = sc.nextInt();
        Solution obj = new Solution();
        System.out.println(obj.maxConsecutiveOnes(arr, n));
    }
}


