//-------------------Bubble Sort Problem---------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int a[]=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                int count=0;
                      boolean flag=false;
                for(int i=0; i<n-1; i++){
                        
                      for(int j=0; j<(n-i-1);j++){
                              if(a[j]>a[j+1]){
                                      flag=true;
                                      count++;
                                      int temp=a[j];
                                      a[j]=a[j+1];
                                      a[j+1]=temp;
                              }
                      }
                        if(flag==false){
                                break;
                        }
                }
                System.out.println("Array is sorted in "+ count+" swaps." );
                System.out.println("First Element: "+a[0]);
                System.out.println("Last Element: "+a[n-1]);
                
                        
	}
}

