import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
                int h1=sc.nextInt();
                int h2=sc.nextInt();
                int v1=sc.nextInt();
                        int v2=sc.nextInt();
               if(v1==v2){
                       System.out.println("false");
               }
                else{
                int n=h2-h1;
                int d=v1-v2;

                
                        if(n%d==0){
                                System.out.println("true");
                        }
                        else{
                                System.out.println("false");
                        }

                }
	}
}



