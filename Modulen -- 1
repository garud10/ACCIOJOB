//----------------FACING THE SUN------------------//

import java.util.*;
import java.lang.*;
import java.io.*;

public class Main
{
	public static void main (String[] args) throws java.lang.Exception
	{

           Scanner sc=new Scanner(System.in);
                int n=sc.nextInt();
                int[]a=new int[n];
                for(int i=0; i<n; i++){
                        a[i]=sc.nextInt();
                }
                int count=1;
                int max=a[0];
                for(int i=1; i<n; i++){
                        if(a[i]>max){
                                count++;
                                max=a[i];
                        }
                }
                System.out.println(count);






                
	}
}



